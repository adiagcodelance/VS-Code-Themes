    "tab.activeBorder": "#e95420",
    "tab.activeBorderTop": "#e95420",
    
    "editorWidget.background": "#242424",
    "editorWidget.border": "#e95420",
    "editorSuggestWidget.background": "#242424",
    "editorSuggestWidget.border": "#e95420",
    "editorSuggestWidget.selectedBackground": "#e9542033",
    
    "badge.background": "#e95420",
    "badge.foreground": "#ffffff",
    
    "scrollbarSlider.background": "#4a4a4a66",
    "scrollbarSlider.hoverBackground": "#5a5a5a99",
    "scrollbarSlider.activeBackground": "#6a6a6acc",
    
    "focusBorder": "#e95420",
    "editorGroup.border": "#1a1a1a",
    "editorGroupHeader.tabsBackground": "#242424"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#888888",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#8ae234"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#e95420"
      }
    },
    {
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#fce94f"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#729fcf"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#ad7fa8"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.class"],
      "settings": {
        "foreground": "#34e2e2"
      }
    },
    {
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#e95420"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#ef2929"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#729fcf"
      }
    },
    {
      "scope": ["support.type.property-name"],
      "settings": {
        "foreground": "#729fcf"
      }
    },
    {
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#d3d7cf"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#e95420",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
